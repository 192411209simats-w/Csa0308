#include <stdio.h>

int main() {
    int numbers[] = {5, 8, 13, 22, 41, 60};
    int size = sizeof(numbers) / sizeof(numbers[0]);

    printf("Odd or Even check:\n");

    for (int i = 0; i < size; i++) {
        if (numbers[i] % 2 == 0)
            printf("%d is Even\n", numbers[i]);
        else
            printf("%d is Odd\n", numbers[i]);
    }

    return 0;
}
